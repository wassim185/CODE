ALGORITHM CHERCHE 
VAR
    str:STRING
    tab=names : ARRAY_OF CHAR;
    i:INTEGER=0
    s:INTEGER=0
    c:INTEGER=0
    v=INTEGER=0
    n=tab.length
    BEGIN
    FOR i FROM 0 TO n-1 STEP step  DO
       IF (tab[i]=" ") THEN
        s:=s+1
       END_IF
       IF (tab[i]="a,u,i,e,y,o") THEN
       v:=v+1
       END_IF
       END_FOR 
       END
